<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd ">

    <dubbo:application name="share-im-rpc-service"/>

    <dubbo:registry address="zookeeper://zkserver:2181"/>

    <dubbo:protocol name="dubbo" port="20883"/>

    <!--<dubbo:monitor protocol="registry"/>-->

    <!-- 电子围栏分类 -->
    <bean id="imElectronicfenceclassificationServiceImpl" class="com.szce.im.rpc.service.impl.ImElectronicfenceclassificationServiceImpl"/>
    <dubbo:service interface="com.szce.im.rpc.api.ImElectronicfenceclassificationService" ref="imElectronicfenceclassificationServiceImpl" timeout="10000"/>

    <!-- 电子围栏基础信息 -->
    <bean id="imElectronicfencedataServiceImpl" class="com.szce.im.rpc.service.impl.ImElectronicfencedataServiceImpl"/>
    <dubbo:service interface="com.szce.im.rpc.api.ImElectronicfencedataService" ref="imElectronicfencedataServiceImpl" timeout="10000"/>

    <!-- 自行车道 -->
    <bean id="imBikewayServiceImpl" class="com.szce.im.rpc.service.impl.ImBikewayServiceImpl"/>
    <dubbo:service interface="com.szce.im.rpc.api.ImBikewayService" ref="imBikewayServiceImpl" timeout="10000"/>
    <!-- 标识标志 -->
    <bean id="imLogosignServiceImpl" class="com.szce.im.rpc.service.impl.ImLogosignServiceImpl"/>
    <dubbo:service interface="com.szce.im.rpc.api.ImLogosignService" ref="imLogosignServiceImpl" timeout="10000"/>
</beans>